<?xml version="1.0"?>

<PropertyList>

 <name>ASI</name>
 <path>asi.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>Needle</object-name>
  <emission>
   <red>1.0</red>
   <green>0.2</green>
   <blue>0.0</blue>
   <factor-prop>/sim/model//material/instruments/factor</factor-prop>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Needle</object-name>
  <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
  <interpolation>
   <entry><ind>  10</ind><dep>    3</dep></entry>
   <entry><ind>  40</ind><dep>   25</dep></entry>
   <entry><ind>  60</ind><dep>   77</dep></entry>
   <entry><ind>  80</ind><dep>  129</dep></entry>
   <entry><ind> 100</ind><dep>  181</dep></entry>
   <entry><ind> 120</ind><dep>  233</dep></entry>
   <entry><ind> 140</ind><dep>  285</dep></entry>
   <entry><ind> 160</ind><dep>  337</dep></entry>
   <entry><ind> 170</ind><dep>  357</dep></entry>
   <entry><ind> 200</ind><dep>  359</dep></entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


